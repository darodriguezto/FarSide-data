#include<iostream>

int main(int argc, char **argv)
{
  int month=std::atoi(argv[1]);
  int day;
  int hour;
  for (int ii=0;ii<62;++ii)
    {
      day=(ii+2)/2;
      hour=12*(ii%2);
        if (day==0 && hour ==0)
          {
            std::cout << "AR_LIST_2022."<<month<<".01_00:00:00.txt"<< std::endl;
          }
        if (day<=10){
          if (hour==0){
            std::cout <<"AR_LIST_2022."<<month<<".0"<< day<< "_0"<<hour<< ":00:00.txt" <<std::endl;
          }
          else
            std::cout <<"AR_LIST_2022."<<month<<".0"<< day<< "_"<<hour<< ":00:00.txt" << std::endl;
        }
        else {
          if (hour==0){
            std::cout <<"AR_LIST_2022."<<month<<"."<< day<< "_0"<<hour<< ":00:00.txt" <<std::endl;
          }
          else
            std::cout <<"AR_LIST_2022."<<month<<"."<< day<< "_"<<hour<< ":00:00.txt" <<std::endl;
        }}
  return 0;
}
